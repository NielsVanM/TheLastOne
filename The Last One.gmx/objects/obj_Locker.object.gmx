            <string>///Hiding in locker
if (locker = false)
{
    if (keyboard_check_pressed(vk_space))
    {
        if (place_meeting(x,y+1,obj_Player))
        {
            if (global.hide = false)
            {
                //Adjusting view
                view_hspeed[0] = 2;
                view_vspeed[0] = 2;
                
                //Storing x and y player
                lastx = obj_Player.x;
                lasty = obj_Player.y;
                
                //Set player inside locker
                obj_Player.x = x;
                obj_Player.y = y;
                
                //Hide the player
                global.hide = true
                
                //Sprites
                if (texture = 0){sprite_index = spr_Locker1_Hide;}
                if (texture = 1){sprite_index = spr_Locker2_Hide;}
                if (texture = 2){sprite_index = spr_Locker3_Hide;}
            }
            else
            {
                //Setting player x and y back
                obj_Player.x = lastx;
                obj_Player.y = lasty;
                //Unhide the player
                global.hide = false;
                
                //Resetting sprites
                if (texture = 0){sprite_index = spr_Locker1;}
                if (texture = 1){sprite_index = spr_Locker2;}
                if (texture = 2){sprite_index = spr_Locker3;}
                
                //Resetting view
                locker = true;
                alarm[0] = 0.5 * room_speed;
                
                //Setting player sprite to down
                with (obj_Player){sprite_index = spr_Player_Down;}
            }
        }
    }
}
